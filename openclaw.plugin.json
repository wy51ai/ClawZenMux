{
  "id": "clawzenmux",
  "name": "ClawZenMux",
  "description": "Smart LLM router via ZenMux — 100+ models, rules-based routing, token cost savings",
  "configSchema": {
    "type": "object",
    "properties": {
      "apiKey": {
        "type": "string",
        "description": "ZenMux API key. Optional — reads from ZENMUX_API_KEY env var if not set."
      },
      "routing": {
        "type": "object",
        "description": "Override default routing configuration",
        "properties": {
          "tiers": {
            "type": "object",
            "description": "Customize which model handles each complexity tier",
            "properties": {
              "SIMPLE": {
                "type": "object",
                "properties": {
                  "primary": { "type": "string", "description": "Model ID for simple tasks (e.g. deepseek/deepseek-chat)" }
                }
              },
              "MEDIUM": {
                "type": "object",
                "properties": {
                  "primary": { "type": "string", "description": "Model ID for medium tasks" }
                }
              },
              "COMPLEX": {
                "type": "object",
                "properties": {
                  "primary": { "type": "string", "description": "Model ID for complex tasks" }
                }
              },
              "REASONING": {
                "type": "object",
                "properties": {
                  "primary": { "type": "string", "description": "Model ID for reasoning tasks" }
                }
              }
            }
          }
        }
      }
    }
  },
  "uiHints": {
    "apiKey": {
      "label": "ZenMux API Key",
      "sensitive": true,
      "placeholder": "sk-... (optional — reads from ZENMUX_API_KEY env var)"
    }
  }
}
